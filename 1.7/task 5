//5. Написать программу, которая выводит таблицу квадратов десяти первых положительных чисел.
# include <iostream>
# include <cmath>
using namespace std;

int main() {
    int i;
    for (i=1; i <= 10; i++) {
        cout << "the square of the number " << i << " = " << pow(i, 2) << endl;
    }
    return 0;
}